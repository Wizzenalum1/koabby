<div id = "student-questions">
    <% for(var test in tests){ %>
    <a href="/student/test/?id=<%= tests[test].id %>" class = "question">
        <h1><%= tests[test].title%></h1>
        <div class = "problem statement"> <%= tests[test].subject%></div> 
        <div class = "options">
            <div> <span> Number of questions in the test </span><%=tests[test].numberOfQuestion %></div>
            <div><span>posible max marks in in the test</span><%= tests[test].maxMarks %></div>
            <div><span> time to complete the test.</span><%= tests[test].testDuration %></div>
        </div>        
    </a>
        <% } %>
</div>